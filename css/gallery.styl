 .gallery
   width 100%
   padding 0 100px
   display flex
   flex-direction column
   align-items center

   &__row
     width 100%
     display flex
     justify-content space-between
     height 440px

     .gallery__block
       height 100%
       .gallery__img
         width 100%
         height 100%
         object-fit cover
      /* object-position */

     .--first-block, .--second-block
       width 49%



